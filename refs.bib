	doi = {https://doi.org/10.1016/j.ribaf.2019.101061},
	issn = {0275-5319},
	journal = {Research in International Business and Finance},
	keywords = {Market microstructure, Emerging market, Institutional investors, Individual investors, Trading strategies},
	pages = {101061},
	title = {Who moves the stock market in an emerging country -- Institutional or retail investors?},
	url = {https://www.sciencedirect.com/science/article/pii/S0275531919300327},
	volume = {51},
	year = {2020},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0275531919300327},
	bdsk-url-2 = {https://doi.org/10.1016/j.ribaf.2019.101061}}

@article{AYMO2019e00112,
	abstract = {I examine the cross-sectional relationship between institutional trading activity and daily and intradaily stock returns for a sample of NYSE, Amex, and Nasdaq securities. An analysis of the prior day's return sort indicates that, institutional investors are 13.6 percent more likely to be net buyers of securities that are in the winner (top performing decile) decile than of those that are in the loser decile. There is a strong daily contemporaneous relation between institutional imbalances and returns, and institutional trades follow the previous day's returns. The intradaily analysis suggests that the contemporaneous relation is primarily driven by institutional trades following the intradaily prices. There is no evidence of institutional return predictability.},
