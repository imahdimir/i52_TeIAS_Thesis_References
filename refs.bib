	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/0022-1082.00184},
	journal = {The Journal of Finance},
	number = {6},
	pages = {2143-2184},
	title = {A Unified Theory of Underreaction, Momentum Trading, and Overreaction in Asset Markets},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/0022-1082.00184},
